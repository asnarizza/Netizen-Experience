{"ast":null,"code":"import AppHeader from './components/AppHeader.vue';\nimport CatSwipeStack from './components/CatSwipeStack.vue';\nimport CatSummary from './components/CatSummary.vue';\nimport { useCats } from './composables/useCats';\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    /**\n     * Root component: owns the cat list, loading/error state,\n     * and the high-level navigation between swipe view and summary view.\n     */\n    const {\n      cats,\n      currentIndex,\n      isLoading,\n      error,\n      isFinished,\n      likedCats,\n      loadCats,\n      handleSwipe\n    } = useCats();\n\n    /**\n     * Reload handler used by both error state and summary \"restart\" button.\n     */\n    function reload() {\n      loadCats();\n    }\n\n    /**\n     * Adapter for the swipe event emitted by CatSwipeStack.\n     * The child emits { isLike }, we pass just the boolean into the composable.\n     */\n    function onSwipe(payload) {\n      handleSwipe(payload.isLike);\n    }\n    const __returned__ = {\n      cats,\n      currentIndex,\n      isLoading,\n      error,\n      isFinished,\n      likedCats,\n      loadCats,\n      handleSwipe,\n      reload,\n      onSwipe,\n      AppHeader,\n      CatSwipeStack,\n      CatSummary,\n      get useCats() {\n        return useCats;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["AppHeader","CatSwipeStack","CatSummary","useCats","cats","currentIndex","isLoading","error","isFinished","likedCats","loadCats","handleSwipe","reload","onSwipe","payload","isLike"],"sources":["C:/Users/User/netizen_experience/src/App.vue"],"sourcesContent":["<template>\n  <!-- Root app container: full-page flexbox with centered content -->\n  <div class=\"app\">\n    <!-- Header: shows title and progress summary -->\n    <AppHeader\n      :isFinished=\"isFinished\"\n      :likedCount=\"likedCats.length\"\n      :totalCount=\"cats.length\"\n    />\n\n    <!-- Loading state -->\n    <div v-if=\"isLoading\" class=\"center-box\">\n      <p>Loading purr-fect kittiesâ€¦</p>\n    </div>\n\n    <!-- Error state -->\n    <div v-else-if=\"error\" class=\"center-box\">\n      <p class=\"error\">{{ error }}</p>\n      <button @click=\"reload\" class=\"btn primary\">Try again</button>\n    </div>\n\n    <!-- Main content (either swipe view or summary) -->\n    <main v-else class=\"content\">\n      <!-- Swipe UI while there are still cats to rate -->\n      <CatSwipeStack\n        v-if=\"!isFinished\"\n        :cats=\"cats\"\n        :currentIndex=\"currentIndex\"\n        @swipe=\"onSwipe\"\n      />\n\n      <!-- Summary screen once all cats have been rated -->\n      <CatSummary\n        v-else\n        :likedCats=\"likedCats\"\n        @restart=\"reload\"\n      />\n    </main>\n  </div>\n</template>\n\n<script setup>\n/**\n * Root component: owns the cat list, loading/error state,\n * and the high-level navigation between swipe view and summary view.\n */\nimport AppHeader from './components/AppHeader.vue';\nimport CatSwipeStack from './components/CatSwipeStack.vue';\nimport CatSummary from './components/CatSummary.vue';\nimport { useCats } from './composables/useCats';\n\nconst {\n  cats,\n  currentIndex,\n  isLoading,\n  error,\n  isFinished,\n  likedCats,\n  loadCats,\n  handleSwipe\n} = useCats();\n\n/**\n * Reload handler used by both error state and summary \"restart\" button.\n */\nfunction reload() {\n  loadCats();\n}\n\n/**\n * Adapter for the swipe event emitted by CatSwipeStack.\n * The child emits { isLike }, we pass just the boolean into the composable.\n */\nfunction onSwipe(payload) {\n  handleSwipe(payload.isLike);\n}\n</script>\n\n<style scoped>\n/* Root app layout: full height, vertical flex, soft gradient background */\n.app {\n  min-height: 100vh;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  background:\n    radial-gradient(circle at 0% 0%, #ffe6f2 0, transparent 50%),\n    radial-gradient(circle at 100% 100%, #e0f4ff 0, transparent 50%),\n    linear-gradient(180deg, #fef9f6, #fdf2ff);\n  color: #222;\n  padding: 2rem 1rem;\n  box-sizing: border-box;\n}\n\n/* Shared max-width for main content so layout stays consistent */\n.content {\n  width: 100%;\n  max-width: 360px;\n  flex: 1;\n  display: flex;\n  flex-direction: column;\n  align-items: center;\n  justify-content: center;\n  padding-bottom: 2rem;\n}\n\n/* Error message styling */\n.error {\n  color: #b00020;\n}\n</style>\n"],"mappings":"AA8CA,OAAOA,SAAS,MAAM,4BAA4B;AAClD,OAAOC,aAAa,MAAM,gCAAgC;AAC1D,OAAOC,UAAU,MAAM,6BAA6B;AACpD,SAASC,OAAO,QAAQ,uBAAuB;;;;;;;;IAP/C;AACA;AACA;AACA;IAMA,MAAM;MACJC,IAAI;MACJC,YAAY;MACZC,SAAS;MACTC,KAAK;MACLC,UAAU;MACVC,SAAS;MACTC,QAAQ;MACRC;IACF,CAAC,GAAGR,OAAO,CAAC,CAAC;;IAEb;AACA;AACA;IACA,SAASS,MAAMA,CAAA,EAAG;MAChBF,QAAQ,CAAC,CAAC;IACZ;;IAEA;AACA;AACA;AACA;IACA,SAASG,OAAOA,CAACC,OAAO,EAAE;MACxBH,WAAW,CAACG,OAAO,CAACC,MAAM,CAAC;IAC7B","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}